.widget-body-toolbar { 
	background: white;
}

.fixed-header-table-wrapper {
	overflow-x: auto;
}

.table-fixed-header {
	min-width: 550px;
	margin-bottom: 0;
}
.custom-scroll { 
	min-width: 600px;
	max-height: 160px; 
	overflow-y: auto !important; 
} 


.table-bordered th, .table-bordered td {
	min-width: 35px !important;
}

.table-bordered th {
	text-align: center !important;
	border-left-width: 0 !important; 
    border-bottom-width: 0 !important;
}
.tooltip-inner {
	font-size: 14px; 
	text-align: left; 
	max-width: 400px; 
}
.tooltip-inner .unread_mcht {
	cursor: pointer; padding: 2px;
}


.stat_title { 
	padding-bottom: 10px; 
	font-weight: bold;
}
.cs-css-01 { 
	padding: 13px 13px !important;
}
.cs-css-01.red { 
	color: #ff6565 !important;
}


.nav-tabs > li.active > a , .nav-tabs > li.active > a:hover {
	-webkit-box-shadow: 0 -2px 0 #4c4f53; 
	box-shadow: 0px -2px 0 #4c4f53; 
}

