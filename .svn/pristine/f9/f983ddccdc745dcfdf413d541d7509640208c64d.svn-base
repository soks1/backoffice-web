function f_appoint(obj) {
	if(!obj.checked) {
		$("#prod_ck_cnt").attr("disabled", true);
		//$("#prod_ck_cnt option:eq(0)").attr("selected", true);
		$("#prod_ck_cnt").val("0");
		f_appointDisplayNum(0);
		f_missionChannelChange();
	}
	
	var missionTypeCnt = $("input[name='bsis_mison_yn_list']").length;
	if(missionTypeCnt > 1) {
		alert('미션 유형은 최대 2개까지 가능합니다.');
		obj.checked = false;
		return;
	}
	
	if(obj.checked) {
		$("#prod_ck_cnt").attr("disabled", false);
		$("#prod_ck_cnt").removeAttr("disabled");
	}
}

/* ============================================================================================
/* 특정지역방문 체크박스
/* --------------------------------------------------------------------------------------------
/* 특정지역방문 체크박스 클릭시 호출
/* 기존 추가된 다른미션이 2개이면 체크 안됨.
/* 체크 TRUE	: 특정지역방문 미션 정보 입력 레이아웃 추가 
