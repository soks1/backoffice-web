function f_typeComma(obj) {
	
	var val = $('#coupon_type_cd option:selected').val();
	
	if(val == 'A' || val == 'F' || val == 'C' || val == 'H' || val == 'J' || val == 'L' 
			|| val == 'N' || val == 'P' || val == 'T' || val == 'R' || val == 'V') {
		return $(obj).val();
	}
	else {
		return cf_fmtComma(obj);
	}
}

