<form id="frmKeepData">
<%
	Enumeration<?> e = request.getParameterNames();
	while(e.hasMoreElements()){
		String key = (String)e.nextElement();
		String values[] = request.getParameterValues(key);
		for(int i = 0; i < values.length; i++) {
			out.println("<input type=\"hidden\" name=\""+LangUtil.replaceInjection(key)+"\" value=\""+LangUtil.replaceInjection(values[i])+"\" />\n");	
		}
	}
%>
</form>
<script type="text/javascript">
/**
* ==================================================================================
* 노출위치 : M클럽 값에 따른 UI 변경
* ==================================================================================
*/
$('select[name=mclub_id]').change(function(){
	var _mclub_id = $(this).val();
	var options =$("select[name=view_location");
	
	if(_mclub_id == $("#baby_club_id").val()) {
		options.children().remove();
		options.append("<option value='ALL'> 선택 </option>");
	}else if(_mclub_id == $("#vic_club_id").val()) {
		$("select[name=view_location").children().remove();
		$("select[name=view_location").append("<option value='ALL'> 전체 </option>");
		f_ajaxCouponlocation($("select[name=view_location]"));
		
		// VIC M클럽 노출위치
		$('select[name=view_location]').children("[value='COUPON']").remove();
		$('select[name=view_location]').children("[value='MCLUB']").remove();
	}else {
		options.children().remove();
		options.append("<option value=''> 선택 </option>");		
	}	
});

/**
 * ==================================================================================
 * 달력 : 종료일이 시작일을 넘길 수 없도록 셋팅
 * ==================================================================================
 */
$('#ntce_start_dd').datepicker(
    $.extend({
        onSelect: function(selected) {
            $('#ntce_end_dd').datepicker('option','minDate', selected);
        }
    }, date_opts)
);
$('#ntce_end_dd').datepicker(
    $.extend({
        onSelect: function(selected) {
            $('#ntce_start_dd').datepicker('option','maxDate', selected);
        }
    }, date_opts)
);

/**
 * ==================================================================================
 * 달력 : 현재날짜 시간/분
 * ==================================================================================
 */
function cf_toHoursMins() {
	var to = new Date();
	var hour = to.getHours();
	var min = to.getMinutes();
		
	if(parseInt(hour) < 10) {
		hour = '0'+parseInt(hour);
	}
	if(parseInt(min) < 10) {
		min = '0'+parseInt(min);
	}
	return hour + '-' + min;
}

/* ============================================================================================
/* 연결 방법에 따른 URL 
/* --------------------------------------------------------------------------------------------
/* radio 선택에 따른 visible 변경
/* ============================================================================================ */
function f_applyType(link) {
	$("input[name=ot_link_url]").val("");
	if(link == 'APPLIST') {
		$('#linkUrlA').show();
		$('#linkUrlB').hide();
	}else if (link == 'URL'){
		$('#linkUrlB').show();
		$('#linkUrlA').hide();
	}else if (link == 'USELESS'){
		$('#linkUrlB').hide();
		$('#linkUrlA').hide();
	}
}
/* ============================================================================================
/* APP 내부 페이지 연결
/* --------------------------------------------------------------------------------------------
/* 배너 클릭시 연결될 내부게시판 호출
/* ============================================================================================ */
$('#noticbd_divn_cd_temp').change(function(){
	
	if($('#mclub_custgp_id').val() == "") {
		alert("M클럽을  먼저 선택해 주세요.");
		$("#noticbd_divn_cd_temp").val("");
		return;
	}
	
	var _divn_cd = $(this).val();
	
	if(_divn_cd != "") {
		var data = $('#frmMclubEvent').serializeFormObject();
		data.empno = "${sessionScope.sessionEmpno}";
		data.link_url = _divn_cd;
		
		if(_divn_cd != "LIST") {
			$("#outside_url").hide();
			$("#inside_url").show();
			$('#zine_ord_seq').hide();
			
			if(_divn_cd == "EVENTCOK" || _divn_cd == "CHANNELCOK") {
				var _url = "";
				if(_divn_cd == "EVENTCOK") {
					_url = "/backoffice/common/board/issue/event/cok";
				}else {
					_url = "/backoffice/common/board/issue/channel/cok";
				}
				
				$.ajax({
					url: api_url + _url,
	// 				type: 'GET', // 기본이 GET 임 주석처리.
	// 				cache: true, // 기본이 true 임 주석처리.
					async: false,
					data: encodeURI('params='+JSON.stringify(data)),
					success: function(obj){
						var objData = obj['data'] || {};
						$('select[name=link_noticbd_id]').data({'items': objData['result_list'], 'key': 'evnt_id', 'label': 'evnt_nm', '': function() {}}).triggerHandler('init');
					},beforeSend:function(){
						$("select[name=link_noticbd_id] option[value!='']").remove();
					},error: function(obj) {
				    	var serverObj = obj.responseJSON;
				    	alert('code:'+serverObj.code+'\n'+'message:'+cf_msg(serverObj.message)+'\n');
				    	return;
				    }
				});
			} else if ( _divn_cd == $("#baby_club_id").val() ){
				var target = $('select[name=link_noticbd_id]');
				var options = "<option value=''>선택</option>";
				
				options +="<option value=CRM>우리아이M클럽 쿠폰</option>";
				options +="<option value=POLL>우리아이M클럽 설문</option>";
				options +="<option value=CPZINE>육아 TIP</option>";
				options +="<option value=STAMP>스탬프 이벤트</option>";
				
				target.html(options);				
			} else if ( _divn_cd == $("#vic_club_id").val() ){
				var target = $('select[name=link_noticbd_id]');
				var options = "<option value=''>선택</option>";
				
				options +="<option value=DM/LEAFLET>행사상품</option>";
				options +="<option value=VIC_GUIDE>쇼핑TIP</option>";
				options +="<option value=STAMP>스탬프 이벤트</option>";
				options +="<option value=CRM>VIC클럽 쿠폰</option>";
				target.html(options);
			} else {
				var data = {
						empno: "${sessionScope.sessionEmpno}", 
						emp_str_cd: "${sessionScope.sessionStrCd}",
						hq_base_yn: "${sessionScope.sessionHqBaseYn}",
				 		link_url : _divn_cd,
				 		lst_custgp_id_list : [$("input[name=mclub_custgp_id]").val()]
			
				};
				$.ajax({
					url: api_url+"/backoffice/couponbook/issue/cpbook/list",
					type: 'GET',
					async: false,
					cache: true,
					data: encodeURI('params='+JSON.stringify(data)),
					success: function(obj) {
						var obj = obj.data;
						$('#zine_ord_seq').show();
						var target = $('select[name=link_noticbd_id]');
						var options = "<option value=''>선택</option>";
						if(obj.url_type=="CRM") {
							$.each(obj.result_list, function(i, item) {
								options += "<option value='"+item.evnt_id+"'data-mapp-digit='"+item.cpbook_mapp_digit+"'data-cpbook-cd='"+item.ctype+"'data-cpbook-id='"+item.evnt_id+"'data-cpbook-type-cd='"+item.cpbook_type_cd+"'data-cpbook-cptype-cd='"+item.c_type+"'>"+item.evnt_nm+"</option>";
							});
						}else if(obj.url_type=="CPZINE") {
							$.each(obj.result_list, function(i, item) {
								options += "<option value='"+item.evnt_id+"'data-mapp-digit='"+item.cpbook_mapp_digit+"'data-cpbook-cd='"+item.ctype+"'data-cpbook-id='"+item.evnt_id+"'data-cpbook-type-cd='"+item.cpbook_type_cd+"'data-cpbook-cptype-cd='"+item.c_type+"'>"+item.evnt_nm+"</option>";
							});
						}else if(obj.url_type=="STAMP") {
							$.each(obj.result_list, function(i, item) {
								options += "<option value='"+item.evnt_id+"'>"+item.evnt_nm+"</option>";
							});
						}else if(obj.url_type=="FCFS") {
							$.each(obj.result_list, function(i, item) {
								options += "<option value='"+item.evnt_id+"'>"+item.evnt_nm+"</option>";
							});
						}
						target.html(options);
					},beforeSend:function(){
						$("select[name=link_noticbd_id] option[value!='']").remove();
					},error: function(obj) {
				    	var serverObj = obj.responseJSON;
				    	alert('code:'+serverObj.code+'\n'+'message:'+cf_msg(serverObj.message)+'\n');
				    	return;
				    }
				});
			}
		} else {
			$("#zine_ord_seq").hide();
			$("select[name=zine_ord_seq] option:selected").val("");

			var target = $('select[name=link_noticbd_id]');
			var options = "<option value=''>선택</option>";
			
