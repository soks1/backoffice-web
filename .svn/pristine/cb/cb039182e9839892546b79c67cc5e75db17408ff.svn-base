	disable: true,
	title : "<div class='widget-header'><h6><i class='fa fa-pencil-square-o'></i> 쿠폰북 선택 </h6></div>",
	buttons : [{
		html : "<i class='fa fa-save'></i>&nbsp; 적용",
		"class" : "btn btn-primary",
		click : function() {
			
			$("#to_tbody").find("tr").each(function(){
				
				var tag_td = "";
				var key = "";
				var chk = 0;
				
				// 루프문을 돌면서 Append 할 Tag 생성, index가 1인 위치에서 Key가 될 값 추출
				$(this).find("td").each(function(i){
					if(i == 1) {
						return true;
					}
					
					// 각 셀 데이터 생성
					if(i < 6) {
						if(i == 2) {
							key = $(this).text();
							tag_td += "<input type='hidden' name='cpbook_id_list' data-array-always='true' value='"+key+"'>";
						}
						tag_td += "<td class='ta-c'>"+$(this).html()+"</td>\n";	
					}
				});
// 				tag_td += "<td class='ta-c input-icon-right'><select id='open_date_list' name='open_date_list' data-array-always='true' class='form-control'><option value='0'>D-0</option><option value='1'>D-1</option><option value='3'>D-3</option><option value='5'>D-5</option><option value='7'>D-7</option><option value='10'>D-10</option><option value='15'>D-15</option></select></td>";
				tag_td += "<td class='ta-c input-icon-right'><input type='text' id='open_date_list' name='open_date_list' data-valid-use='true' data-array-always='true' class='form-control' onkeyup='cf_numChk(this);' data-valid-title='노출설정일' style='padding:0 0 0 2px; text-align:center' value='0' maxlength='3' /></td>";
				tag_td += "<td class='ta-c input-icon-right'><i class='fa fa-calendar' style='padding-right:10px'></i><input class='form-control datepicker' name='vali_term_start_dd_list' data-valid-use='true' onchange='cf_chg(this);' data-array-always='true' data-date-format='yyyyMMdd' type='text' data-dateformat='yy-mm-dd' data-valid-title='시작일' placeholder='시작일'></td>";
				tag_td += "<td><input type='text' name='vali_term_start_time_list' data-valid-use='true' data-array-always='true' class='form-control' onkeyup='cf_numChk2(this);' data-valid-title='시작시간' style='padding:0 0 0 2px; text-align:center' maxlength='2' value='00' /></td>";
				tag_td += "<td class='ta-c input-icon-right'><i class='fa fa-calendar' style='padding-right:10px'></i><input class='form-control datepicker' name='vali_term_end_dd_list' data-valid-use='true' data-array-always='true' data-date-format='yyyyMMdd' type='text' data-dateformat='yy-mm-dd' data-valid-title='종료일' placeholder='종료일'></td>";
				tag_td += "<td><input type='text' name='vali_term_end_time_list' data-valid-use='true' data-array-always='true' class='form-control' onkeyup='cf_numChk2(this);' data-valid-title='종료시간' style='padding:0 0 0 2px; text-align:center' maxlength='2' value='23' /></td>";

				var tag_tr = "<tr>"+tag_td+"</tr>";
				
				// 기존 등록 테이블을 돌면서 받은 Key 값과 같은 데이터가 있는지 확인
				$("#couponBook_tbody").find("tr > td").each(function(){
					if($(this).text() == key) {
						chk++;
					}
				});
				
				// 같은 데이터가 없을 경우에만 등록 시킨다.
				if(chk == 0) {
					$("#couponBook_tbody").append(tag_tr);	
				}
				
				$("#cpbook_gbn_span").show();
	    		$('input:radio[name=cpbook_gbn]:input[value=A]').prop("checked", true); // 쿠폰북 구분 (신규추가) (A:일반, B:신데렐라)
	    		$('#couponBook_tbody').find('input[name=vali_term_start_time_list]').each(function(i) {
	    			$(this).val("00");
	    			$("#couponBook_tbody").find("input[name=vali_term_end_time_list]").eq(i).val("23");
	    			$(this).attr('disabled','disabled');
	    			$("#couponBook_tbody").find("input[name=vali_term_end_time_list]").eq(i).attr('disabled','disabled');
	    		});
				
				pageSetUp();
			});
			$(this).dialog("close");
		}
	},{
		html : "<i class='fa fa-times'></i>&nbsp; 닫기",
		"class" : "btn btn-default",
		click : function() {
			$(this).dialog("close");
		}
