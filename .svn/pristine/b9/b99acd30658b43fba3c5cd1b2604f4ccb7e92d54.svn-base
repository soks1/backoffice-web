					$('#gen_img_reg_yn').val("Y"); // 생성이미지 등록여부 Y
			    },
			    error: function(resultData) {
			    	var serverObj = resultData["responseJSON"];
			    	alert('code:'+serverObj["code"]+'\n'+'message:'+cf_msg(serverObj["message"])+'\n');
			    	return;
			    }
			});			

			
			$(this).dialog("close");
