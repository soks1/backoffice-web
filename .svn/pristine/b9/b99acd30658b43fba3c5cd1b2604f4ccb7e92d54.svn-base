			    success: function(resultData){
// 			    	alert(resultData["message"]);
			    	
			    	var data = resultData.data;

					var btn_div = $("#gen_uploadfile").parent().parent();
// 					$('#gen_preview_uploadfile').find('img').attr('src',data.prvw_url);
