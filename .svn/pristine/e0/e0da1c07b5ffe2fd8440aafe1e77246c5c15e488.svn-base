/* ============================================================================================
/* 페이지 정보 설정
/* --------------------------------------------------------------------------------------------
/* 초기 설정 및 필요 UI 컴포넌트 셋팅
/* ============================================================================================ */
var pagefunction = function() {
	var frm_data = $("#frmKeepData").serializeFormObject();

	// POLL 기본 정보
	$.ajax({
	    url: api_url+"/backoffice/poll/show",
	    type: "GET",
	    async: true,
	    cache: true,
	    datatype: 'json',
	    data: encodeURI("params="+JSON.stringify(frm_data)),
	    beforeSend : function() {},
	    success: function(obj){
	    	var data = obj.data;
	    	
	    	// 삭제되거나 POLL 발행된 설문일 경우 수정 불가
	    	if(data.del_yn == 'N') {
	    		$('.btn_auth_update').remove();
	    	}
	    	
	    	$('#poll_type_nm').html(data.poll_type_nm);
	    	$('input[name=poll_att]').val(data.poll_att);
	    	$('input[name=poll_title]').val(data.poll_title);
	    	$('textarea[name=poll_desc]').val(data.poll_desc);
	    	$('textarea[name=intro_msg]').val(data.intro_msg);
	    	$('textarea[name=agree_sbjt]').val(data.agree_sbjt);
	    	$('#reg_name').html(data.reg_name);
	    	$('#reg_date').html(data.reg_date);
	    	$('#mod_name').html(data.mod_name);
	    	$('#mod_date').html(data.mod_date);
	    	
	    	if(data.poll_type_cd == 'SNGL') {
		    	$('#addDelDiv a').eq(0).hide();
		    	$('#addDelDiv a').eq(1).hide();
		    	$('#addDelDiv a').eq(3).hide();
		    	$('#logicManageBtn').hide();
		    	
		    	$('#intro_msg_div').hide();
				$('#agree_sbjt_div').hide();
	    	}
	    	else {
	    		$('#intro_msg_div').show();
				$('#agree_sbjt_div').show();
	    	}
	    	
	    	POLL_TYPE_CD = data.poll_type_cd;
	    	INTRO_MSG 	 = data.intro_msg;
	    	
	    	// POLL 문항 리스트 조회
	    	f_pollQstnList();
	    },
	    error: function(obj) {
	    	var serverObj = obj.responseJSON;
	    	alert("code:"+serverObj.code+"\n"+"message:"+cf_msg(serverObj.message)+"\n");
	    	return;
	    }
	});
};

/* ============================================================================================
/* 페이지 셋업 (Don't remove!!!)
/* --------------------------------------------------------------------------------------------
/* 기본 페이지 사용 컴포넌트 호출 및 초기화 작업 진행 
/* ============================================================================================ */
pageSetUp();

/* ============================================================================================
/* 페이지 정보 설정 로드
/* --------------------------------------------------------------------------------------------
/* 컴포넌트 SCRIPT 파일 로드 및 페이지 정보 설정 SCRIPT 로드
/* ============================================================================================ */
pagefunction();

</script>
