function f_openCouponCampaignSD() {
	$.ajax({
		url: "/application/crm/campaignWrite.do",
		type: 'GET',
		async: true,
		cache: false,
		success: function(data) {
			var $target = $('#coupon_campaign_sd');
			$target.html(data);
		},
		complete: function() {
			$('#coupon_campaign_sd').dialog('open');
		}
	});
}

/**
 * 페이지 전환 (타겟 지정 가능)
 * @param url
 * @param method
 * @param data
 * @param target
 */
function cf_actionTarget(url, method, data, target) {
	
	$.ajax({
		
	    url: url, //this is the submit URL
	    type: method, // GET or POST
	    async: true,
	    cache: false,
	    data: data,
	    beforeSend : function() {},
	    success: function(html){
	    	target.html(html);
	    },
	    error: function() {
	    	ajaxError('ERR','901');
	    }
	});
}

/**
 * 페이지 전환 (타겟 고정:targetHtml)
 * @param url
 * @param method
 * @param data
 */
function cf_mCouponAction(url, method, data) {
	$.ajax({
	    url: url, //this is the submit URL
	    type: method, // GET or POST
	    async: true,
	    cache: false,
	    data: data,
	    beforeSend : function() {},
	    success: function(html){
	    	$("#targetHtml").html(html);
	    },
	    error: function() {
	    	ajaxError('ERR','901');
	    },
	    complete: function() {}
	});
}
