		/*}else if(_issue_cpbook == "MLIFE") {
			url = rn_url + "#/mlifelist";*/
		}else if(_issue_cpbook == "EVENTCOK") {
			url = rn_url + "#/eventlist";
		}else if(_issue_cpbook == "LIFECOK") {
			url = rn_url + "#/lifekokmain";
		}else if(_issue_cpbook == "CHANNELCOK") {
