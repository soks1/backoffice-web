		cache: false,
	    datatype: 'json',
	    contentType: 'application/json;charset=utf-8',
		data: JSON.stringify(sendData),
		success: function(data) {
			if(data.data.ccode == "S000"){
				alert("알림 등록이 완료되었습니다.");
//				$("#crmSendWrite").dialog("close");
