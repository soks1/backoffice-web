		}else if(_issue_cpbook == "POLL") {
			url = "application/poll/pollMainLN.html";
		}else if(_issue_cpbook == "PEVENT") {
			url = "application/event/pedometer/pedometerMainLN.html";
		}else if(_issue_cpbook == "") {
			
		}
	}else if(url_type == "OUTSIDE") {
		var out_url = $("#outside_url").find("input[name=issue_cpbook]").val();
		
//		if (!out_url.startswith("http://")){
//			out_url = "http://" + out_url;
//		}
//		if (out_url.substring(0, 7) != "http://" || out_url.substring(0, 8) == "https://"){
//			out_url = "http://" + out_url;
//		}
		
		if ( out_url.indexOf("http://") < 0 && out_url.indexOf("https://") < 0 ) {
			out_url = "http://" + out_url; 
		}
		
//		url = "application/main/main.html#/?out_url=" + out_url;
		
