	}
	if($('input[name=userYn]:checked').val() == "Z") {
		if( $("#gen_img_reg_yn").val() != "Y" ){
			alert("이미지를 등록해주세요.");
			return;
		}
	}
	
	if($('input[name=linkYn]:checked').val() == "Y") {
		if($('select[name=link_url]').val() == "") {
			alert("연결할 URL을 선택해 주세요.");
