function f_pushSend() {
	var url_type = $('select[name=link_url]').val();	
	var data_mapp_digit = $("select[name=issue_cpbook] option:selected").attr("data-mapp-digit");
	var data_cpbook_id = $("select[name=issue_cpbook] option:selected").attr("data-cpbook-id");
