		$('#crm_title').focus();
		$('.push_send').removeClass('disabled');
		return;
	}
	if ($("#crm_desc").val() == '' || $("#crm_desc").val()== null) {
		alert('내용을 입력하세요');
		$('#crm_desc').focus();
		$('.push_send').removeClass('disabled');
		return;
	}
	
	if($('input[name=userYn]:checked').val() == "N" || $('input[name=userYn]:checked').val() == "Z") {
		if( !$("#preview_uploadfile").next().hasClass("alert")){
			alert("이미지를 등록해주세요.");
