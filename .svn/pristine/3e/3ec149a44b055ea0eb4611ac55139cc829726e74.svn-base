/* M클럽 : 클럽 고객군
/* --------------------------------------------------------------------------------------------
/* 
/* ============================================================================================ */
function f_ajaxCustgpStatusCode(target, type) {
	var data = {
			empno: "${sessionScope.sessionEmpno}", 
			str_cd: "${sessionScope.sessionStrCd}",
			hq_base_yn: "${sessionScope.sessionHqBaseYn}",
			cd_divn_id: "LST_CUSTGP_ID",
			type: type,
			mst_cd : ""
	};
	
	$.ajax({
		url: api_url + "/backoffice/mclub/custgroup/list",
		type: "GET",
		async: false,
		data: encodeURI('params='+JSON.stringify(data)),
		success: function(obj) {
			var options = "";
			$.each(obj.data.custgp_list, function(i, item) {
				options += "<option value='"+item.lst_temp_custgp_id+"'>"+item.lst_temp_custgp_id+"</option>";
			});
			target.append(options);
		},
		error: function(obj) {
	    	var serverObj = obj.responseJSON;
	    	alert("code:"+serverObj.code+"\n"+"message:"+cf_msg(serverObj.message)+"\n");
	    	return;
	    }
	});
}

