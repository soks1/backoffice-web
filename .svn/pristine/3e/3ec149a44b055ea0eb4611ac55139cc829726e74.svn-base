/* FAQ : FAQ 분류 조회 스크립트
/* --------------------------------------------------------------------------------------------
/* FAQ 분류 : 쿠폰/스탬프/경매/공동구매/전자영수증
/* LAYER - L : 리스트 화면 사용 
/* LAYER - W : 등록 화면 사용
/* ============================================================================================ */
function f_ajaxFaqCatCode(target, layer) {
		
	var data = {
			
			empno: '${sessionScope.sessionEmpno}', 
			str_cd: '${sessionScope.sessionStrCd}',
			hq_base_yn: '${sessionScope.sessionHqBaseYn}',
			cd_divn_id: "FAQ_CAT_CD",
			mst_cd : ""
	}; 
	
	$.ajax({
		url: api_url + "/backoffice/common/code/list",
		type: "GET",
		async: false,
		data: encodeURI('params='+JSON.stringify(data)),
		success: function(obj) {
			var options = "";
			if(layer == 'L') {
				options += "<option value=''>전체</option>";
			} else if(layer == 'W'){
				options += "<option value=''>선택</option>";
			}
			$.each(obj.data.cd_list, function(i, item) {
				if(layer == 'W' && item.mst_cd == 'COUPON') {
					options += "<option value='"+item.mst_cd+"' selected='selected'>"+item.mst_cd_nm+"</option>";
				} else {
					options += "<option value='"+item.mst_cd+"'>"+item.mst_cd_nm+"</option>";
				}
				
			});
			target.html(options);
		},
		error: function(obj) {
	    	var serverObj = obj.responseJSON;
	    	alert("code:"+serverObj.code+"\n"+"message:"+cf_msg(serverObj.message)+"\n");
	    	return;
	    }	
	});
}

/* ============================================================================================
/* 공통 : 등록지점 조회
/* --------------------------------------------------------------------------------------------
/* 
/* ============================================================================================ */
function f_ajaxStrCdCode(target, type, ordType) {
	
	var svcType = "CPBOOK";
	if(typeof type != "undefined" && type != null) {
		svcType = type;
	}
	
	if('${sessionScope.sessionHqBaseYn}' == 'Y') {
		var data = {
				
				svc_type: svcType,
				empno: "${sessionScope.sessionEmpno}", 
				str_cd: "${sessionScope.sessionStrCd}",
				hq_base_yn: "${sessionScope.sessionHqBaseYn}",
				use_type: "BRANCH",
				mst_cd : "", 
				ord_dype : ordType
		};
		
		$.ajax({
			url: api_url + "/backoffice/common/branch/list",
			type: "GET",
			async: false,
			data: encodeURI('params='+JSON.stringify(data)),
			success: function(obj) {
				var options = "";
				$.each(obj.data.brnch_list, function(i, item) {
					options += "<option value='"+item.str_cd+"'>"+item.str_nm+"</option>";
				});
				target.append(options);
			},
			error: function(obj) {
		    	var serverObj = obj.responseJSON;
		    	alert("code:"+serverObj.code+"\n"+"message:"+cf_msg(serverObj.message)+"\n");
		    	return;
		    }
		});
	} else {
		target.html('<option value="${sessionScope.sessionStrCd}">${sessionScope.sessionStrNm}</option>');
	}

}



