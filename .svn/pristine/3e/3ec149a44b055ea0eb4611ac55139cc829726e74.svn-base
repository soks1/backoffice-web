/* 쿠폰 : 자/타사 구분 코드 가져오기
/* --------------------------------------------------------------------------------------------
/* 자/타사 구분 코드 조회 스크립트
/* ============================================================================================ */
function f_ajaxCompanyType(target) {
	var data = {
			empno: "${sessionScope.sessionEmpno}", 
			str_cd: "${sessionScope.sessionStrCd}",
			hq_base_yn: "${sessionScope.sessionHqBaseYn}",
			cd_divn_id: "COMPANY_TYPE_CD",
			mst_cd : ""
	};
	
	$.ajax({
	    url: api_url+"/backoffice/common/code/list",
		type: "GET",
		async: false,
		data: encodeURI('params='+JSON.stringify(data)),
		success: function(obj) {
			var options = "";
			$.each(obj.data.cd_list, function(i, item) {
				options += "<option value='"+item.mst_cd+"'>"+item.mst_cd_nm+"</option>";
			});
			target.append(options);
		},
		error: function(obj) {
	    	var serverObj = obj["responseJSON"];
	    	alert("code:"+serverObj["code"]+"\n"+"message:"+cf_msg(serverObj["message"])+"\n");
	    	return;
	    }
	});
}

$.getSessionData = function() {
	return {
		access_token: '${sessionScope.sessionEmpToken}',
		empno		: '${sessionScope.sessionEmpno}',
		str_cd		: '${sessionScope.sessionStrCd}',
		hq_base_yn	: '${sessionScope.sessionHqBaseYn}'
	};
};

</script>