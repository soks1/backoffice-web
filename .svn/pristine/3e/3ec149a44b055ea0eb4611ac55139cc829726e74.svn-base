/* ============================================================================================
/* 쿠폰 : 쿠폰 유형 조회 스크립트
/* --------------------------------------------------------------------------------------------
/* 적용대상 유형에 따른 쿠폰 유형 변경 스크립트
/* LAYER - L : 리스트 화면 사용 
/* LAYER - W : 등록 화면 사용
/* TYPE	- PRICE		: 정률할인/정액할인/정률적립/정액할인
/* TYPE	- PRODUCT 	: 정률할인/정액할인/정률적립/정액할인/정액할인(포인트쿠폰)
/* ============================================================================================ */
function f_ajaxCouponTypeCode(target, layer, type) {
	if(typeof type == "undefined" || type == null || type == "") {
		type = "A";
	}

	var data = {
			
			empno: '${sessionScope.sessionEmpno}', 
			str_cd: '${sessionScope.sessionStrCd}',
			hq_base_yn: '${sessionScope.sessionHqBaseYn}',
			cd_divn_id: "COUPON_TYPE_CD",
			mst_cd : type
	}; 
	
	$.ajax({
		url: api_url + "/backoffice/common/code/list",
		type: "GET",
		async: false,
		data: encodeURI('params='+JSON.stringify(data)),
		success: function(obj) {
			var options = "";
			if(layer == 'L') {
				options += "<option value=''>전체</option>";
			} else if(layer == 'W'){
				options += "<option value=''>선택</option>";
			}
			$.each(obj.data.cd_list, function(i, item) {
				options += "<option value='"+item.mst_cd+"'>"+item.mst_cd_nm+"</option>";
			});
			target.html(options);
		},
		error: function(obj) {
	    	var serverObj = obj.responseJSON;
	    	alert("code:"+serverObj.code+"\n"+"message:"+cf_msg(serverObj.message)+"\n");
	    	return;
	    }	
	});
}

